st time via the advanced electron microscopy.","abstract_source":"pubmed","altmetric_jid":"4f6fa62f3cf058f610008259","authors":["Yang Sun","Liang Zhao","Huilin Pan","Xia Lu","Lin Gu","Yong-Sheng Hu","Hong Li","Michel Armand","Yuichi Ikuhara","Liquan Chen","Xuejie Huang"],"doi":"10.1038/ncomms2878","endpage":"","first_seen_on":"2013-05-23T13:45:00+00:00","issns":["2041-1733"],"journal":"Nature Communications","links":["http://www.nature.com/ncomms/journal/v4/n5/full/ncomms2878.html"],"pmid":"23695664","pubdate":"2013-05-21T00:00:00+00:00","publisher":"Nature Publishing Group","publisher_subjects":[{"name":"Multidisciplinary","scheme":"era"}],"startpage":"1870","title":"Direct atomic-scale confirmation of three-phase storage mechanism in Li4Ti5O12 anodes for room-temperature sodium-ion batteries","type":"article","mendeley_url":"http://www.mendeley.com/research/direct-atomicscale-confirmation-threephase-storage-mechanism-litio-anodes-roomtemperature-sodiumion"},"altmetric_score":{"score":0.5,"score_history":{"1y":0,"6m":0,"3m":0,"1m":0,"1w":0,"6d":0,"5d":0,"4d":0,"3d":0,"2d":0,"1d":0,"at":0.5},"context_for_score":{"all":{"total_number_of_other_articles":5036908,"mean":5.9657355758162,"rank":3555679,"this_scored_higher_than_pct":16,"this_scored_higher_than":833910,"rank_type":"exact","sample_size":5036908,"percentile":16},"similar_age_3m":{"total_number_of_other_articles":94214,"mean":7.2749725834016,"rank":63931,"this_scored_higher_than_pct":17,"this_scored_higher_than":16377,"rank_type":"exact","sample_size":94214,"percentile":17},"this_journal":{"total_number_of_other_articles":8213,"mean":33.672298343887,"rank":7297,"this_scored_higher_than_pct":5,"this_scored_higher_than":435,"rank_type":"exact","sample_size":8213,"percentile":5},"similar_age_this_journal_3m":{"total_number_of_other_articles":321,"mean":23.1663125,"rank":278,"this_scored_higher_than_pct":7,"this_scored_higher_than":23,"rank_type":"exact","sample_size":321,"percentile":7}}},"demographics":{"poster_