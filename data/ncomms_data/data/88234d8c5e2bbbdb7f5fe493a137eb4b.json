iltonian.","abstract_source":"pubmed","altmetric_jid":"4f6fa62f3cf058f610008259","authors":["Hebin Li","Alan D. Bristow","Mark E. Siemens","Galan Moody","Steven T. Cundiff"],"doi":"10.1038/ncomms2405","endpage":"","first_seen_on":"2013-01-25T20:00:42+00:00","issns":["2041-1723"],"journal":"Nature Communications","links":["http://www.nature.com/ncomms/journal/v4/n1/full/ncomms2405.html"],"pmid":"23340430","pubdate":"2013-01-22T00:00:00+00:00","publisher":"Nature Publishing Group","publisher_subjects":[{"name":"Multidisciplinary","scheme":"era"}],"startpage":"1390","subjects":["biology","science"],"title":"Unraveling quantum pathways using optical 3D Fourier-transform spectroscopy","type":"article","mendeley_url":"http://www.mendeley.com/research/unraveling-quantum-pathways-using-optical-3d-fouriertransform-spectroscopy"},"altmetric_score":{"score":1,"score_history":{"1y":0,"6m":0,"3m":0,"1m":0,"1w":0,"6d":0,"5d":0,"4d":0,"3d":0,"2d":0,"1d":0,"at":1},"context_for_score":{"all":{"total_number_of_other_articles":4728273,"mean":5.7012211078408,"rank":2513728,"this_scored_higher_than_pct":33,"this_scored_higher_than":1570805,"rank_type":"exact","sample_size":4728273,"percentile":33},"similar_age_3m":{"total_number_of_other_articles":287409,"mean":6.2047151923399,"rank":142659,"this_scored_higher_than_pct":38,"this_scored_higher_than":111855,"rank_type":"exact","sample_size":287409,"percentile":38},"this_journal":{"total_number_of_other_articles":7537,"mean":31.600178078556,"rank":5991,"this_scored_higher_than_pct":13,"this_scored_higher_than":1055,"rank_type":"exact","sample_size":7537,"percentile":13},"similar_age_this_journal_3m":{"total_number_of_other_articles":277,"mean":20.328514492754,"rank":187,"this_scored_higher_than_pct":23,"this_scored_higher_than":65,"rank_type":"exact","sample_size":277,"percentile":23}}},"demographics":{"poster_types":{"member_of_the_public":1},"users":{"twitter":{"cohorts":{"Members of the public":1}},"mendeley":{"by_status":{"Student  > B