nductor ferroelectrics.","altmetric_jid":"4f6fa62f3cf058f610008259","authors":["Wei-Qiang Liao","Wei-Qiang Liao","Yi Zhang","Chun-Li Hu","Jiang-Gao Mao","Heng-Yun Ye","Peng-Fei Li","Songping D. Huang","Ren-Gen Xiong"],"doi":"10.1038/ncomms8338","first_seen_on":"2015-05-30T20:55:53+00:00","funders":["niehs"],"issns":["2041-1723"],"journal":"Nature Communications","links":["http://www.nature.com/ncomms/2015/150529/ncomms8338/full/ncomms8338.html"],"pmid":"26021758","pubdate":"2015-05-29T00:00:00+00:00","publisher":"Nature Publishing Group","publisher_subjects":[{"name":"Multidisciplinary","scheme":"era"}],"subjects":["biology","science"],"title":"A lead-halide perovskite molecular ferroelectric semiconductor","type":"article","volume":"6","mendeley_url":"http://www.mendeley.com/research/leadhalide-perovskite-molecular-ferroelectric-semiconductor"},"altmetric_score":{"score":0.5,"score_history":{"1y":0,"6m":0,"3m":0,"1m":0,"1w":0,"6d":0,"5d":0,"4d":0,"3d":0,"2d":0,"1d":0,"at":0.5},"context_for_score":{"all":{"total_number_of_other_articles":5165331,"mean":6.0999753905388,"rank":3627364,"this_scored_higher_than_pct":16,"this_scored_higher_than":870719,"rank_type":"exact","sample_size":5165331,"percentile":16},"similar_age_3m":{"total_number_of_other_articles":172831,"mean":8.8391975814385,"rank":123103,"this_scored_higher_than_pct":17,"this_scored_higher_than":30919,"rank_type":"exact","sample_size":172831,"percentile":17},"this_journal":{"total_number_of_other_articles":8467,"mean":34.35501015828,"rank":7527,"this_scored_higher_than_pct":4,"this_scored_higher_than":370,"rank_type":"exact","sample_size":8467,"percentile":4},"similar_age_this_journal_3m":{"total_number_of_other_articles":722,"mean":32.607134535368,"rank":657,"this_scored_higher_than_pct":4,"this_scored_higher_than":32,"rank_type":"exact","sample_size":722,"percentile":4}}},"demographics":{"poster_types":{"member_of_the_public":1},"users":{"twitter":{"cohorts":{"Members of the public":1}},"mendeley":{"by_